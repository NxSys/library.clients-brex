{
    "name": "nxsys/library.clients-brex",
    "description": "PHP SDK for Brex",
    "type": "library",
    "homepage": "http://nxsys.org/spaces/onx",
	"license": "MIT",
	"authors": [
		{
			"name": "Nexus Systems, Inc.",
			"email": "onx@Nxs.Systems"
		}
	],
	"autoload": {
		"psr-4": {
			"NxSys\\Library\\Clients\\Brex\\API\\": "src-gen/",
			"NxSys\\Library\\Clients\\Brex\\": "src/"
		}
	},
    "require": {
        "psr/http-message": "^1.0",
        "psr/http-client-implementation": "*",
        "php-http/httplug": "^2.3",
        "php-http/discovery": "^1.14",
        "jane-php/open-api-runtime": "^7.4",
        "nyholm/psr7": "^1.7"
    },
    "require-dev": {
        "symfony/http-client": "^6.2",
        "php-http/mock-client": "^1.5",
        "jane-php/open-api-3": "^7.4",
        "friendsofphp/php-cs-fixer": "^3.13",
        "php-http/logger-plugin": "^1.3",
        "monolog/monolog": "^3.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
